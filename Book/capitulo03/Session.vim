if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +1 booknotes03.gnuplot
badd +1 records
argglobal
%argdel
$argadd booknotes03.gnuplot
$argadd records
set stal=2
tabnew +setlocal\ bufhidden=wipe
tabrewind
edit booknotes03.gnuplot
argglobal
